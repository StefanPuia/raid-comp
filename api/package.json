{
  "name": "raid-comp-api",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "start": "ts-node src/index.ts",
    "start:watch": "cross-env NODE_ENV=local ts-node-dev --respawn --transpile-only src/index.ts",
    "start:in:docker": "cross-env NODE_ENV=production node dist/index.js",
    "build": "npm run clean && tsc && npm run swagger",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --collect-coverage",
    "test:watch": "nodemon --exec cross-env NODE_ENV=test jest",
    "swagger": "swaggerGen -c ./swagger.config.yml"
  },
  "dependencies": {
    "@google-cloud/datastore": "^6.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "10.0.0",
    "express": "^4.17.1",
    "gstore-node": "^7.2.6",
    "morgan": "1.10.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^4.1.3",
    "passport-jwt": "^4.0.0",
    "sha256": "^0.2.0",
    "typescript-rest": "^3.0.4",
    "typescript-rest-swagger": "^1.1.7",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node-cron": "^2.0.4",
    "@types/node-fetch": "^2.5.12",
    "@types/passport-jwt": "^3.0.6",
    "@types/request": "^2.48.7",
    "@types/sha256": "^0.2.0",
    "@types/underscore": "^1.11.3",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.0.6",
    "jest-html-reporter": "^3.4.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
