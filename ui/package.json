{
  "name": "raid-comp-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "yarn test:integration && yarn test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "prepare": "yarn smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui-light.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@haverstack/axios-fetch-adapter": "0.12.0",
    "@material/typography": "^14.0.0",
    "@playwright/test": "^1.28.1",
    "@smui-extra/bottom-app-bar": "^7.0.0-beta.15",
    "@smui/button": "^7.0.0-beta.15",
    "@smui/card": "^7.0.0-beta.15",
    "@smui/circular-progress": "^7.0.0-beta.15",
    "@smui/common": "^7.0.0-beta.15",
    "@smui/dialog": "^7.0.0-beta.15",
    "@smui/fab": "^7.0.0-beta.15",
    "@smui/icon-button": "^7.0.0-beta.15",
    "@smui/layout-grid": "^7.0.0-beta.15",
    "@smui/list": "7.0.0",
    "@smui/menu": "^7.0.0-beta.15",
    "@smui/paper": "^7.0.0-beta.15",
    "@smui/segmented-button": "^7.0.0-beta.15",
    "@smui/select": "7.0.0",
    "@smui/snackbar": "^7.0.0-beta.15",
    "@smui/textfield": "^7.0.0-beta.15",
    "@smui/tooltip": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "2.3.4",
    "@sveltejs/kit": "^1.27.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^4.0.5",
    "svelte-check": "^3.6.0",
    "svelte-turnstile": "0.6.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.34.0"
  },
  "type": "module",
  "dependencies": {
    "svelte-i18n": "^4.0.0"
  }
}
