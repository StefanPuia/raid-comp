{
  "name": "raid-comp-api",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc",
    "build": "npm run clean && npm run compile && npm run swagger",
    "start:watch": "cross-env NODE_ENV=local ts-node-dev --respawn --transpile-only src/index.ts",
    "start:in:docker": "node dist/index.js",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --collect-coverage",
    "test:watch": "nodemon --exec cross-env NODE_ENV=test jest",
    "swagger": "swaggerGen -c ./swagger.config.yml"
  },
  "dependencies": {
    "@google-cloud/datastore": "6.6.2",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "gstore-node": "8.0.0",
    "morgan": "1.10.0",
    "node-cron": "3.0.2",
    "node-fetch": "2.6.7",
    "node-html-parser": "6.1.5",
    "passport-jwt": "4.0.1",
    "sha256": "0.2.0",
    "typescript-rest": "3.0.4",
    "typescript-rest-swagger": "1.1.7",
    "underscore": "1.13.6"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/morgan": "1.9.4",
    "@types/node-cron": "3.0.7",
    "@types/node-fetch": "2.6.2",
    "@types/passport-jwt": "3.0.8",
    "@types/request": "2.48.8",
    "@types/sha256": "0.2.0",
    "@types/underscore": "1.11.4",
    "@typescript-eslint/parser": "5.53.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.4.3",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "29.4.3",
    "jest-html-reporter": "3.7.0",
    "request": "2.88.2",
    "rimraf": "4.1.2",
    "source-map-support": "0.5.21",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  }
}
